{
  "name": "garden-planner",
  "version": "0.0.0",
  "description": "A planner to plan where to plant plants and record when they were planted and when they should be harvested",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "serve": "webpack-dev-server",
    "build:sass": "node-sass ./src/sass/main.scss public/assets/style.css --source-map-contents",
    "watch:sass": "node-sass ./src/sass/main.scss public/assets/style.css --source-map-contents -w",
    "prod:sass": "node-sass ./src/sass/main.scss public/assets/style.css --output-style compressed",
    "postcss": "postcss -r ./public/assets/style.css"
  },
  "keywords": [
    "Garden",
    "Planner"
  ],
  "author": "Stephen James <spjames95@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "autoprefixer": "^9.8.0",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.31",
    "postcss-cli": "^7.1.1",
    "postcss-focus-within": "^4.0.0",
    "source-map-loader": "^1.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "firebase": "^7.14.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
